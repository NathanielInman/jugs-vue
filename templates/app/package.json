{
  "name": "<%= name %>",
  "version": "<%= appVersion %>",
  "description": "<%= appDescription %>",
  "main": "config.babel.js",
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= userName %>/<%= name %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= userName %>/clay/issues"
  },
  "scripts": {
    "build:clean": "rm -rf ./dist && mkdir ./dist",
    "build:assets": "cp -rf src/assets/* ./dist",
    "build": "npm run build:clean && npm run build:assets",
    "prestart": "npm run build",
    "start": "NODE_ENV=development node ./node_modules/webpack/bin/webpack.js --config ./config.babel.js --watch"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "estraverse-fb": "^1.3.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "html-webpack-template-pug": "^1.1.1",
    "node-sass": "^4.5.3",
    "poststylus": "^1.0.0",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "sass-loader": "^6.0.6",
    "source-loader": "^1.0.0",
    "source-map-loader": "^0.2.2",
    "style-loader": "^0.19.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "buefy": "^0.5.3",
    "ion-cloud": "^1.1.1",
    "vue": "^2.4.4",
    "vue-router": "^2.7.0"
  },
  "readmeFilename": "README.md",
  "homepage": "https://github.com/<%= userName %>/<%= name %>"
}
